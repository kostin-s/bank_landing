{
  "singleQuote": true,
  "trailingComma": "all",
  "jsxSingleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/shared/(.*)$",
    "^@/hooks/(.*)$",
    "^@/assets/(.*)$",
    "^../(.*)$",
    "^./(.*)$",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
